{
    "name": "React Demo",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "React Chrome Extension",
    "homepage_url": "http://emptycan.com",
    "icons": { 
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png" 
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "19": "icon19.png",
            "38": "icon38.png"
        },
        "default_title": "React Chrome Extension"
    },
    "permissions": [
        "http://*/",
        "https://*/",
        "activeTab",
        "storage"
    ]
}